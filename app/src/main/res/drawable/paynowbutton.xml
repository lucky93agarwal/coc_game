<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#68EE0E"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#8C1E25"
                android:width="2dp"/>
            <gradient android:startColor="#37610B"
                android:endColor="#C2DF18"
                android:centerColor="#70A011"
                android:angle="90"/>
        </shape>
    </item>

</layer-list>